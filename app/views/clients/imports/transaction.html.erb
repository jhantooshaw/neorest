<h2 class="sub-header">Import Transaction Tables</h2>
<%= form_tag import_transaction_path, multipart: true, class: "form-horizontal", id: "formID" do %>
    <div class="form-group">   
      <label for="import" class="col-sm-3 control-label" style="font-weight:normal;">Attach Excel File For Transaction Import</label>
      <div class="col-sm-3">
        <input type="file" name="import_file" class="filestyle validate[required, custom[checkExcelFile]]" data-icon="false" data-size="sm"  data-buttonBefore="true">
      </div>
    </div>

    <div class="form-group">
      <div class="col-sm-offset-3 col-sm-3">
        <button type="submit" class="btn btn-primary import_button">&nbsp;&nbsp;Import&nbsp;&nbsp;</button>
      </div>
    </div>
  <%end%>